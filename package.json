{
  "name": "antigravity-yolo-mode",
  "displayName": "Antigravity YOLO Mode",
  "description": "Auto-accept terminal commands in Antigravity (does not auto-approve plans)",
  "version": "1.0.2",
  "publisher": "freddyjda1",
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "icon": "icon.png",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/freddyjda/antigravity-yolo-mode"
  },
  "contributes": {
    "commands": [
      {
        "command": "yolo.toggle",
        "title": "YOLO: Toggle Auto-Accept"
      }
    ]
  },
  "scripts": {
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "package": "vsce package --no-yarn"
  },
  "devDependencies": {
    "@types/node": "20.2.5",
    "@types/vscode": "^1.80.0",
    "typescript": "^5.1.3",
    "@vscode/vsce": "^2.19.0"
  }
}